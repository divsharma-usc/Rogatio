var el="#button_"+<%=@question_id%>
console.log(el);
<% if @is_upvoted %>
 $(el).html("Downvote | "+<%=Upvote.where(upvoteable_id:@question_id,upvoteable_type:"question").length.to_s%>);
 noty({text: 'Question upvoted', type: 'alert'})
 <% else %>
 $(el).html("Upvote | "+<%=Upvote.where(upvoteable_id:@question_id,upvoteable_type:"question").length.to_s%>);
 noty({text: 'Tweet downvoted', type: 'alert'});
 <% end %> 